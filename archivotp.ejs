EN MI PERFIL.EJS
<!DOCTYPE html>
<html lang="en">

<%-include("./partials/head")%>

<body>
    <%-include("./partials/header")%>

    <main>
        <div class="user-detail container">
            <section class="top main-content">
                <img class="profile" src="https://via.placeholder.com/100x100" alt="">
                <div class="info-usuario">
                    <p><%= usuario.email %></p> 
                    <a class="follow" href="/editarPerfil"><i class="fa-solid fa-user-gear"></i></a>
                    <ul class="meta">
                        <li>2 posts</li>
                        <li>200 seguidores</li>
                        <li>300 seguidos</li>
                    </ul>
                </div>
            </section>
            <section class="bottom">
                <% for( let i = 0; i < posteos.length; i++ ) { %>
                    <article class="post">
                        <a href="/users/detallePosteo">
                            <img src="<%= posteos.nombre_imagen %>">
                        </a>
                    </article>
                    <ul>
                        <% for( let j = 0; j < posteos.comentario.length; j++ ) { %>
                            <li>
                                <a href="/users/detalleUsuario<%= posteos[i].comentario[j].id_usuario %>"><%= posteos[i].comentario[j].email_usuario %></a>
                                <span><%= posteos[i].comentario[j].texto_comentario %></span>
                            </li>
                        <% } %>
                    </ul>
                <% } %>
            </section>
        </div>
    </main>

    <%-include("./partials/footer")%>
</body>

</html>

